{
	"name": "@aptos-labs/docs-code-cache",
	"version": "0.0.0",
	"description": "Code caching for Github permalinks for Nextra docs",
	"scripts": {
		"test:types": "tsc --noEmit",
		"dev": "wrangler dev",
		"migrate": "wrangler d1 execute db --local --file=./drizzle/0000_short_lockheed.sql",
		"worker:deploy": "wrangler deploy",
		"db:deploy": "wrangler d1 execute db --remote --file=./drizzle/0000_short_lockheed.sql",
		"generate": "drizzle-kit generate:sqlite",
		"up": "drizzle-kit up:sqlite",
		"test": "vitest"
	},
	"keywords": [],
	"author": "hariria",
	"license": "Apache-2.0",
	"private": true,
	"dependencies": {
		"drizzle-orm": "0.30.9",
		"itty-router": "^5.0.16",
		"itty-router-extras": "^0.4.6"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.2.3",
		"@cloudflare/workers-types": "^4.20240423.0",
		"@types/itty-router-extras": "^0.4.3",
		"better-sqlite3": "^9.5.0",
		"drizzle-kit": "^0.20.17",
		"typescript": "^5.4.5",
		"vitest": "1.3.0",
		"wrangler": "^3.52.0"
	}
}
